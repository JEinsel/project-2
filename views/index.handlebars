<video id="bgvid" playsinline autoplay muted loop>
  <source src="https://static.videezy.com/system/resources/previews/000/007/006/original/MR8_8170.mp4"
    type="video/webm">
</video>

<nav class="navbar navbar-expand-lg navbar-dark bg-black fixed-top ">
  <div class="container">
    <a class="navbar-brand" href="/">N.E.Gym</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavDropdown"
      aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavDropdown">
      <ul class="navbar-nav mr-auto">
        <li class="nav-item active">
          <a class="nav-link" href="/">Home <span class="sr-only">(current)</span></a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/about">About</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/amenities">Amenities</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/classes">Classes</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/instructors">Instructors</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/trial">Trial</a>
        </li>
      </ul>

      {{#if user}}
      <span class="navbar-text">
        <a href="/account">Account</a> / <a href="/logout">Log Out</a>
      </span>
      {{/if}}
      {{#unless user}}
      <span class="navbar-text">
        <a href="/login">Login</a> / <a href="/signin">Sign In</a>
      </span>
      {{/unless}}
    </div>
  </div>
</nav>

<section class="container text-center vh-100">
  <h1 class="list py-3">{{ title }}</h1>
  <h2 class="py-3">Become The Person You Want To Be</h2>
  <div class="card-deck">
    <div class="card text-white bg-primary mb-3">
      <div class="card-header h3">Become a member!</div>
      <div class="card-body">
        <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's
          content.</p>
        <h2 class="py-2">$9.99 per month</h2>
        <button class="btn btn-warning">Join Now!</button>
      </div>
    </div>
    <div class="card text-white bg-info mb-3">
      <div class="card-header h3">Gold Membership</div>
      <div class="card-body">
        <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's
          content.</p>
        <h2 class="py-2">$19.99 per month</h2>
        <button class="btn btn-warning">Join Now!</button>
      </div>
    </div>
    <div class="card text-white bg-success mb-3">
      <div class="card-header h3">Buy One Day Pass!</div>
      <div class="card-body">
        <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's
          content.</p>
        <h2 class="py-2">$2.99</h2>
        <button class="btn btn-warning">Join Now!</button>
      </div>
    </div>
  </div>
</section>

