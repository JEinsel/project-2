<section class="container-fluid " id="amenities">
  <div class="container text-center">

    <h1 class="second py-3">{{ title1 }}</h1>


    <div class="card-columns">
      {{#each am}}
      <div class="card">
        <img class="card-img-top" src="{{ photo }}" alt="Card image cap">
        <div class="card-body">
          <h5 class="card-title">{{ name }}</h5>
        </div>

      </div>
      {{/each}}
    </div>
    <div class="card-columns">
      <h1>{{title2}}</h1>
      {{#each inst}}
      <div class="card">
        <img class="card-img-top" src="{{ photo }}" alt="Card image cap">
        <div class="card-body">
          <h5 class="card-title">{{ email }}</h5>
        </div>

      </div>
      {{/each}}
    </div>
  </div>
</section>
